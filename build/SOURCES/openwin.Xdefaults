! @(#)Xdefaults	35.1 94/09/01	OpenWindows Default X Resources
!
OpenWindows.WindowColor:			#cccccc
OpenWindows.WindowForeground:			#000000
OpenWindows.DataForeground:			#000000
OpenWindows.DataBackground:			#ffffff
OpenWindows.PaintWorkspace:			True
OpenWindows.WorkspaceStyle:			paintcolor
OpenWindows.WorkspaceColor:			#264cbf
OpenWindows.WorkspaceBitmapFile:		gray
OpenWindows.WorkspaceBitmapFg:			#000000
OpenWindows.WorkspaceBitmapBg:			#ffffff
OpenWindows.Use3DFrames:			True
OpenWindows.DragWindow:				True
!
OpenWindows.Scale:				medium
!
OpenWindows.ScreenSaver.OnOff:			Auto
OpenWindows.ScreenSaver.IdleTime:		30
OpenWindows.Beep:				never
OpenWindows.BeepDuration:			100
OpenWindows.SetInput:   			followmouse
OpenWindows.ScrollbarPlacement:			right
OpenWindows.IconLocation:			bottom
!
OpenWindows.ScrollbarJumpCursor:		True
Scrollbar.JumpCursor:				True
OpenWindows.PopupJumpCursor:			True
OpenWindows.MultiClickTimeout:			4
OpenWindows.MouseAcceleration:			4
OpenWindows.MouseThreshold:			15
OpenWindows.PointerMapping:			right
!
OpenWindows.KeyboardCommands:			Full
OpenWindows.KeyClick:				False
OpenWindows.KeyRepeat:				True
!
OpenWindows.DragRightDistance:			100
OpenWindows.SelectDisplaysMenu:			False
OpenWindows.MenuAccelerators:			True
OpenWindows.WindowMenuAccelerators:		True
!
olvwm.AllowArrowInRoot:				True
olvwm.ServerGrabs:				False
olvwm.UseImages:				UseAll
olvwm.VirtualBackgroundMap:			/usr/openwin/share/include/images/VirtualDesktop
olvwm.VirtualGridColor:				blue
olvwm.VirtualGrid:				visible
olvwm.GrabVirtualKeys:				False
olvwm.VirtualSticky:				perfmeter clock audiocontrol
text.maxDocumentSize:				500000
!
